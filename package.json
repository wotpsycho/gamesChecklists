{
  "name": "games-checklists",
  "version": "1.0.0",
  "private": true,
  "license": "UNLICENSED",
  "type": "module",
  "description": "Google Apps Script checklist with prerequisites",
  "scripts": {
    "build": "rollup -c",
    "push": "npm run build && node scripts/clasp-deploy.js push",
    "push:all": "npm run build && node scripts/clasp-deploy.js --all",
    "push:add": "node scripts/clasp-deploy.js add",
    "push:list": "node scripts/clasp-deploy.js list",
    "watch": "rollup -c -w",
    "watch:push": "node scripts/watch-push.js"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/google-apps-script": "^2.0.8",
    "rollup": "^4.57.1",
    "rollup-plugin-cleanup": "^3.2.1",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3"
  }
}
